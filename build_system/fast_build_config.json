{
  "app_info": {
    "name": "ECBot",
    "version": "1.0.0",
    "main_script": "main.py",
    "icon_macos": "ECBot.icns",
    "icon_windows": "ECBot.ico"
  },
  "data_files": {
    "directories": [
      "resource", "config", "bot", "gui", "common", "utils",
      "agent", "tests", "knowledge", "settings", "skills",
      "telemetry", "gui_v2/dist", "mcp", "memory", "message_manager",
      "prompts", "runner", "build_system", "hooks", "my_rais_extensions"
    ],
    "files": [
      "app_context.py", "ECBot.ico", "ECBot.icns",
      "ecbot.qm", "ecbot_zh.qm", "role.json", "uli.json", ".env",
      "build.py", "main.py"
    ]
  },
  "pyinstaller": {
    "excludes": [
      "jupyter", "pytest", "tornado", "bokeh", "plotly", "test",
      "setuptools", "distutils", "pip", "wheel", "setuptools_scm",
      "IPython", "ipykernel", "notebook", "jupyter_client", "jupyter_core",
      "matplotlib", "seaborn", "dash", "streamlit",
      "django", "gunicorn", "turtle", "turtledemo",
      "pytest", "coverage", "tox", "black", "flake8", "mypy",
      "sphinx", "docutils", "pydoc", "doctest"
    ]
  },
  "build_modes": {
    "fast": {
      "debug": true,
      "console": true,
      "clean": false,
      "optimize": false,
      "onefile": false,
      "strip": false,
      "upx": false
    }
  },
  "installer": {
    "enabled": true,
    "app_name": "ECBot",
    "app_version": "1.0.0",
    "app_publisher": "ECBot Team",
    "default_dir": "{autopf}\\ECBot",
    "default_group": "ECBot",
    "privileges_required": "lowest",
    "compression": "zip",
    "solid_compression": false,
    "compression_modes": {
      "fast": {
        "compression": "zip",
        "solid_compression": false,
        "internal_compress_level": "fast"
      }
    }
  }
}