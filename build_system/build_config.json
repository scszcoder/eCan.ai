{
  "app": {
    "name": "eCan",
    "version": "1.0.0",
    "description": "eCan AI Assistant - Intelligent automation platform",
    "author": "eCan.AI Team",
    "entry_point": "main.py",
    "icons": {
      "macos": "eCan.icns",
      "windows": "eCan.ico"
    },
    "console": false,
    "debug": false
  },
  "build": {
    "data_files": {
      "directories": [
        "resource",
        "config",
        "bot",
        "gui",
        "common",
        "utils",
        "agent",
        "knowledge",
        "settings",
        "skills",
        "telemetry",
        "gui_v2/dist",
        "ota",
        "third_party/ms-playwright"
      ],
      "files": [
        "app_context.py",
        "eCan.ico",
        "eCan.icns",
        "ecbot.qm",
        "ecbot_zh.qm",
        "role.json"
      ]
    },
    "pyinstaller": {
      "collect_all": [
        "scipy",
        "numpy",
        "pandas",
        "sklearn",
        "torch",
        "tiktoken",
        "PySide6.QtWebEngineCore",
        "PySide6.QtWebEngineWidgets"
      ],
      "collect_data_only": [
        "playwright",
        "browser_use",
        "browser_use.browser",
        "browser_use.agent",
        "browser_use.agent.prompts",
        "agent",
        "crawl4ai",
        "langchain",
        "langgraph",
        "langchain_openai",
        "langchain_mcp_adapters",
        "mcp",
        "anyio",
        "lightrag",
        "lightrag.api",
        "tiktoken_ext",
        "tiktoken_ext.openai_public"
      ],
      "force_includes": [
        "pkg_resources",
        "setuptools.dist",
        "importlib.metadata",
        "importlib_metadata"
      ],
      "force_hiddenimports": [
        "PySide6.QtWebEngineCore",
        "PySide6.QtWebEngineWidgets",
        "PySide6.QtWebChannel",
        "PySide6.QtWebSockets",
        "PySide6.QtWebView",
        "lightrag.api",
        "lightrag.api.config",
        "lightrag.api.lightrag_server",
        "jaraco.text",
        "jaraco.functools",
        "jaraco.context",
        "jaraco.collections",
        "jaraco.itertools",
        "jaraco.classes",
        "jaraco.logging",
        "jaraco.path",
        "jaraco.stream",
        "tiktoken_ext.openai_public",
        "starlette",
        "uvicorn",
        "httpx",
        "httpx_sse",
        "httpcore",
        "h11",
        "httptools",
        "asyncio",
        "asyncio.events",
        "asyncio.futures",
        "asyncio.tasks",
        "threading",
        "concurrent.futures",
        "concurrent.futures.thread",
        "sse_starlette",
        "sse_starlette.sse",
        "fastapi",
        "pydantic",
        "websockets",
        "websocket_client",
        "aiohttp",
        "aiohappyeyeballs",
        "sqlalchemy",
        "PIL",
        "PIL.Image",
        "opencv_python",
        "cv2",
        "selenium",
        "webdriver_manager",
        "openpyxl",
        "xlrd",
        "PyPDF2",
        "pdf2image",
        "cryptography",
        "bcrypt",
        "jwcrypto",
        "logging",
        "colorlog",
        "coloredlogs",
        "dotenv",
        "configobj",
        "configparser",
        "click",
        "typer",
        "rich",
        "tqdm",
        "schedule",
        "psutil",
        "watchdog",
        "watchfiles",
        "qasync",
        "playwright",
        "playwright.sync_api",
        "playwright.async_api"
      ],
      "excludes": [
        "jupyter",
        "pytest",
        "test",
        "tests",
        "testing",
        "pip",
        "wheel",
        "jupyter_client",
        "jupyter_core",
        "django",
        "pysqlite2",
        "MySQLdb",
        "numpy.tests",
        "pandas.tests",
        "sklearn.tests",
        "PIL.tests",
        "email.test",
        "matplotlib.tests",
        "scipy.tests",
        "IPython",
        "notebook",
        "ensurepip",
        "venv",
        "tensorflow",
        "torch.utils.tensorboard"
      ],
      "optimization": {
        "copy_metadata": true
      }
    },
    "modes": {
      "dev": {
        "debug": true,
        "console": true,
        "onefile": false
      },
      "fast": {
        "debug": true,
        "console": true,
        "onefile": false
      },
      "prod": {
        "debug": false,
        "console": false,
        "onefile": false,
        "runtime_tmpdir": {
          "windows": "C:/ecan_cache",
          "macos": "~/Library/Caches/ecan_cache",
          "linux": "/tmp/ecan_cache"
        }
      }
    }
  },
  "platforms": {
    "macos": {
      "codesign": {
        "enabled": false,
        "identity": "Developer ID Application: eCan AI Developer",
        "entitlements": "",
        "exclude_patterns": [
          "**/ms-playwright/**/chromium*/chrome-*/Chromium.app/**",
          "**/ms-playwright/**/chrome*",
          "**/QtWebEngineProcess.app/**"
        ]
      },
      "frameworks": [
        "QtWebEngineCore",
        "QtWebEngineWidgets"
      ],
      "dylib_fixes": [
        "cv2",
        "numpy",
        "scipy",
        "cryptography"
      ],
      "permissions": [
        "screen_recording",
        "accessibility",
        "camera",
        "microphone"
      ]
    },
    "windows": {
      "sign": {
        "enabled": false,
        "tool": "signtool",
        "certificate": "",
        "password": ""
      }
    },
    "linux": {
      "appimage": {
        "enabled": false
      }
    }
  },
  "installer": {
    "enabled": true,
    "app_name": "eCan",
    "app_version": "1.0.0",
    "app_publisher": "eCan.AI Team",
    "default_dir": "{autopf}\\\\eCan",
    "default_group": "eCan",
    "privileges_required": "lowest",
    "compression": "lzma",
    "solid_compression": false,
    "internal_compress_level": "normal",
    "compression_modes": {
      "dev": {
        "compression": "zip",
        "solid_compression": false,
        "internal_compress_level": "normal"
      },
      "prod": {
        "compression": "lzma",
        "solid_compression": true,
        "internal_compress_level": "normal"
      },
      "fast": {
        "compression": "zip",
        "solid_compression": false,
        "internal_compress_level": "fast"
      }
    },
    "windows": {
      "app_id": "{6E1CCB74-1C0D-4333-9F20-2E4F2AF3F4A1}",
      "publisher": "eCan Team",
      "publisher_url": "https://github.com/ecan-team",
      "support_url": "https://github.com/ecan-team/ecbot/issues",
      "updates_url": "https://github.com/ecan-team/ecbot/releases",
      "license_file": "",
      "readme_file": "",
      "icon_file": "eCan.ico",
      "default_dir": "{autopf}\\eCan",
      "default_group": "eCan",
      "privileges_required": "lowest",
      "create_desktop_icon": true,
      "create_quick_launch_icon": false,
      "create_uninstaller": true,
      "compression": "lzma",
      "solid_compression": true,
      "sign_tool": "",
      "sign_params": ""
    },
    "macos": {
      "bundle_identifier": "com.ecan.app",
      "app_name": "eCan",
      "app_version": "1.0.0",
      "copyright": "Copyright Â© 2025 eCan.AI Team",
      "install_location": "/Applications",
      "create_desktop_shortcut": true,
      "create_launchpad_shortcut": true,
      "min_os_version": "11.0",
      "codesign": {
        "enabled": false,
        "identity": "Developer ID Application: eCan AI Developer",
        "entitlements": ""
      },
      "notarization": {
        "enabled": false,
        "apple_id": "",
        "team_id": "",
        "app_password": ""
      },
      "dylib_fixes": [
        "cv2",
        "numpy",
        "scipy",
        "cryptography"
      ],
      "permissions": [
        "screen_recording",
        "accessibility",
        "camera",
        "microphone"
      ]
    }
  },
  "ota": {
    "enabled": true,
    "dependencies_managed_by_ci": true,
    "dependencies_path": "ota/dependencies",
    "fallback_to_http": true,
    "note": "OTA dependencies are installed by CI/CD pipeline, not by build script"
  }
}
