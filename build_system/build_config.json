    "optimization": {
      "copy_metadata": true
    }
  },
  "build_modes": {
    "dev": {
      "debug": true,
      "console": true,
      "onefile": false
    },
    "fast": {
      "debug": true,
      "console": true,
      "onefile": false
    },
    "prod": {
      "debug": false,
      "console": false,
      "onefile": false,
      "runtime_tmpdir": {"windows": "C:/ecan_cache", "macos": "~/Library/Caches/ecan_cache", "linux": "/tmp/ecan_cache"}
    }
  },
  "installer": {
    "enabled": true,
    "app_name": "eCan",
    "app_version": "1.0.0",
    "app_publisher": "eCan.AI Team",
    "default_dir": "{autopf}\\\\eCan",
    "default_group": "eCan",
    "privileges_required": "lowest",
    "compression": "lzma",
    "solid_compression": false,
    "internal_compress_level": "normal",
    "compression_modes": {
      "dev": {
        "compression": "zip",
        "solid_compression": false,
        "internal_compress_level": "normal"
      },
      "prod": {
        "compression": "lzma",
        "solid_compression": true,
        "internal_compress_level": "normal"
      },
      "fast": {
        "compression": "zip",
        "solid_compression": false,
        "internal_compress_level": "fast"
      }
    },
    "windows": {
      "app_id": "{6E1CCB74-1C0D-4333-9F20-2E4F2AF3F4A1}",
      "publisher": "eCan Team",
      "publisher_url": "https://github.com/ecan-team",
      "support_url": "https://github.com/ecan-team/ecbot/issues",
      "updates_url": "https://github.com/ecan-team/ecbot/releases",
      "license_file": "",
      "readme_file": "",
      "icon_file": "eCan.ico",
      "default_dir": "{autopf}\\eCan",
      "default_group": "eCan",
      "privileges_required": "lowest",
      "create_desktop_icon": true,
      "create_quick_launch_icon": false,
      "create_uninstaller": true,
      "compression": "lzma",
      "solid_compression": true,
      "sign_tool": "",
      "sign_params": ""
    },
    "macos": {
      "bundle_identifier": "com.ecan.app",
      "app_name": "eCan",
      "app_version": "1.0.0",
      "copyright": "Copyright Â© 2025 eCan.AI Team",
      "install_location": "/Applications",
      "create_desktop_shortcut": true,
      "create_launchpad_shortcut": true,
      "min_os_version": "11.0",
      "codesign": {
        "enabled": false,
        "identity": "Developer ID Application: eCan AI Developer",
        "entitlements": ""
      },
      "notarization": {
        "enabled": false,
        "apple_id": "",
        "team_id": "",
        "app_password": ""
      },
      "dylib_fixes": [
        "cv2",
        "numpy",
        "scipy",
        "cryptography"
      ],
      "permissions": [
        "screen_recording",
        "accessibility",
        "camera",
        "microphone"
      ]
    }
  },
  "ota": {
    "enabled": true,
    "dependencies_managed_by_ci": true,
    "dependencies_path": "ota/dependencies",
    "fallback_to_http": true,
    "note": "OTA dependencies are installed by CI/CD pipeline, not by build script"
  }
}
