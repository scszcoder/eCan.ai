{
  "app_info": {
    "name": "ECBot",
    "version": "1.0.0",
    "main_script": "main.py",
    "icon_macos": "ECBot.icns",
    "icon_windows": "ECBot.ico"
  },
  "data_files": {
    "directories": [
      "resource", "config", "bot", "gui", "common", "utils",
      "agent", "tests", "knowledge", "settings", "skills",
      "telemetry", "gui_v2/dist", "resource"
    ],
    "files": [
      "app_context.py", "ECBot.ico", "ECBot.icns",
      "ecbot.qm", "ecbot_zh.qm", "role.json", "uli.json", ".env"
    ]
  },
  "pyinstaller": {
    "excludes": [
      "jupyter", "pytest", "test", "pip", "wheel", "jupyter_client", "jupyter_core", "django"
    ]
  },
  "build_modes": {
    "dev": {
      "debug": false,
      "console": true,
      "clean": false,
      "optimize": false,
      "onefile": false
    },
    "prod": {
      "debug": false,
      "console": false,
      "clean": true,
      "optimize": true,
      "onefile": false
    }
  },
  "installer": {
    "enabled": true,
    "app_name": "ECBot",
    "app_version": "1.0.0",
    "app_publisher": "ECBot Team",
    "default_dir": "{autopf}\\\\ECBot",
    "default_group": "ECBot",
    "privileges_required": "lowest",
    "compression": "zip",
    "solid_compression": false,
    "compression_modes": {
      "dev": {
        "compression": "zip",
        "solid_compression": false,
        "internal_compress_level": "fast"
      },
      "prod": {
        "compression": "lzma",
        "solid_compression": true,
        "internal_compress_level": "max"
      },
      "fast": {
        "compression": "zip",
        "solid_compression": false,
        "internal_compress_level": "fast"
      }
    },
    "macos": {
      "bundle_identifier": "com.ecbot.app",
      "install_location": "/Applications",
      "create_desktop_shortcut": false,
      "sign_package": false,
      "developer_id": "",
      "entitlements": "",
      "postinstall_script": true,
      "preinstall_script": false
    }
  }
}
