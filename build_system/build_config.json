{
  "app_info": {
    "name": "ECBot",
    "version": "1.0.0",
    "main_script": "main.py",
    "icon_macos": "ECBot.icns",
    "icon_windows": "ECBot.ico"
  },
  "data_files": {
    "directories": [
      "resource",
      "config",
      "bot",
      "gui",
      "common",
      "utils",
      "agent",
      "tests",
      "knowledge",
      "settings",
      "skills",
      "telemetry",
      "gui_v2/dist",
      "resource"
    ],
    "files": [
      "app_context.py",
      "ECBot.ico",
      "ECBot.icns",
      "ecbot.qm",
      "ecbot_zh.qm",
      "role.json",
      ".env"
    ]
  },
  "pyinstaller": {
    "parallel": true,
    "workers": 0,
    "cache_dir": "build/pyinstaller_cache",
    "collect_data": [
      "transformers",
      "torch",
      "sklearn",
      "pandas",
      "numpy",
      "PIL",
      "cv2",
      "selenium",
      "pydantic",
      "fastapi",
      "uvicorn",
      "jinja2",
      "certifi",
      "charset_normalizer",
      "urllib3",
      "requests",
      "openai",
      "anthropic",
      "langchain",
      "langchain_core",
      "sentence_transformers",
      "browser_use",
      "fake_useragent"
    ],
    "collect_binaries": [
      "torch",
      "cv2",
      "sklearn",
      "scipy",
      "numpy"
    ],
    "collect_submodules": [
      "transformers",
      "langchain",
      "langchain_community",
      "langchain_core",
      "pydantic"
    ],
    "excludes": [
      "jupyter",
      "pytest",
      "test",
      "pip",
      "wheel",
      "jupyter_client",
      "jupyter_core",
      "django",
      "pysqlite2",
      "MySQLdb",
      "numpy.tests",
      "pandas.tests",
      "sklearn.tests",
      "PIL.tests",
      "email.test"
    ]
  },
  "build_modes": {
    "dev": {
      "debug": true,
      "console": true,
      "clean": false,
      "optimize": true,
      "onefile": false,
      "use_cache": false,
      "parallel": true,
      "strip_debug": true,
      "collect_data": "all",
      "collect_binaries": "all",
      "collect_submodules": "all"
    },
    "fast": {
      "debug": true,
      "console": true,
      "clean": true,
      "optimize": false,
      "onefile": false,
      "use_cache": true,
      "parallel": true,
      "strip_debug": true,
      "collect_data": "all",
      "collect_binaries": "all",
      "collect_submodules": "all"
    },
    "prod": {
      "debug": false,
      "console": false,
      "clean": true,
      "optimize": true,
      "onefile": false,
      "use_cache": false,
      "parallel": true,
      "strip_debug": true,
      "collect_data": "all",
      "collect_binaries": "all",
      "collect_submodules": "all"
    }
  },
  "installer": {
    "enabled": true,
    "app_name": "ECBot",
    "app_version": "1.0.0",
    "app_publisher": "ECBot Team",
    "default_dir": "{autopf}\\\\ECBot",
    "default_group": "ECBot",
    "privileges_required": "lowest",
    "compression": "lzma",
    "solid_compression": false,
    "internal_compress_level": "normal",
    "compression_modes": {
      "dev": {
        "compression": "zip",
        "solid_compression": false,
        "internal_compress_level": "normal"
      },
      "prod": {
        "compression": "lzma",
        "solid_compression": true,
        "internal_compress_level": "normal"
      },
      "fast": {
        "compression": "zip",
        "solid_compression": false,
        "internal_compress_level": "fast"
      }
    },
    "macos": {
      "bundle_identifier": "com.ecbot.app",
      "install_location": "/Applications",
      "create_desktop_shortcut": false,
      "sign_package": false,
      "developer_id": "",
      "entitlements": "",
      "postinstall_script": true,
      "preinstall_script": false
    }
  }
  }
