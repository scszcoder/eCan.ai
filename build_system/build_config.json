{
  "app_info": {
    "name": "ECBot",
    "version": "1.0.0",
    "main_script": "main.py",
    "icon_macos": "ECBot.icns",
    "icon_windows": "ECBot.ico"
  },
  "data_files": {
    "directories": [
      "resource", "config", "bot", "gui", "common", "utils",
      "agent", "tests", "knowledge", "settings", "skills",
      "telemetry", "gui_v2/dist", "resource"
    ],
    "files": [
      "app_context.py", "ECBot.ico", "ECBot.icns",
      "ecbot.qm", "ecbot_zh.qm", "role.json", "uli.json", ".env"
    ]
  },
  "pyinstaller": {
    "excludes": [
      "jupyter", "pytest", "tornado", "bokeh", "plotly", "test",
      "setuptools", "distutils", "pip", "wheel", "setuptools_scm",
      "IPython", "ipykernel", "notebook", "jupyter_client", "jupyter_core",
      "matplotlib", "seaborn", "dash", "streamlit",
      "django", "gunicorn", "turtle", "turtledemo"
    ],
    "hidden_imports": [
        "pydantic.deprecated.decorator",
        "langchain_openai",
        "langchain_core",
        "tiktoken",
        "tiktoken_ext",
        "PySide6.QtWebEngineCore",
        "PySide6.QtWebEngineWidgets",
        "PySide6.QtWebChannel",
        "PySide6.QtCore",
        "PySide6.QtGui",
        "PySide6.QtWidgets",
        "PySide6.QtNetwork",
        "qasync",
        "playwright",
        "browser_use",
        "crawl4ai",
        "tkinter",
        "_tkinter",
        "idlelib",
        "scipy",
        "fake_useragent"
    ]
  },
  "build_modes": {
    "dev": {
      "debug": false,
      "console": true,
      "clean": false,
      "optimize": false,
      "onefile": false
    },
    "prod": {
      "debug": false,
      "console": false,
      "clean": true,
      "optimize": true,
      "onefile": false
    }
  },
  "installer": {
    "enabled": true,
    "app_name": "ECBot",
    "app_version": "1.0.0",
    "app_publisher": "ECBot Team",
    "app_publisher_url": "https://github.com/your-repo/ecbot",
    "app_support_url": "https://github.com/your-repo/ecbot/issues",
    "app_updates_url": "https://github.com/your-repo/ecbot/releases",
    "default_dir": "{autopf}\\\\ECBot",
    "default_group": "ECBot",
    "wizard_style": "modern",
    "privileges_required": "lowest",
    "compression_modes": {
      "fast": {
        "compression": "zip",
        "solid_compression": false,
        "internal_compress_level": "fast"
      },
      "dev": {
        "compression": "zip",
        "solid_compression": false,
        "internal_compress_level": "fast"
      },
      "prod": {
        "compression": "lzma",
        "solid_compression": true,
        "internal_compress_level": "max"
      }
    }
  }
}
