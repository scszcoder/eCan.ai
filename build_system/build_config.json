{
  "app_info": {
    "name": "eCan",
    "version": "1.0.0",
    "description": "eCan AI Assistant - Intelligent automation platform",
    "author": "eCan.AI Team",
    "main_script": "main.py",
    "icon_macos": "eCan.icns",
    "icon_windows": "eCan.ico",
    "console": false,
    "debug": false
  },
  "data_files": {
    "directories": [
      "resource",
      "config",
      "bot",
      "gui",
      "common",
      "utils",
      "agent",
      "tests",
      "knowledge",
      "settings",
      "skills",
      "telemetry",
      "gui_v2/dist"
    ],
    "files": [
      "app_context.py",
      "eCan.ico",
      "eCan.icns",
      "ecbot.qm",
      "ecbot_zh.qm",
      "role.json",
      ".env"
    ]
  },
  "pyinstaller": {
    "parallel": true,
    "workers": 0,
    "cache_dir": "build/pyinstaller_cache",
    "distpath": "dist",
    "workpath": "build",
    "specpath": ".",
    "collect_data": [
      "transformers",
      "torch",
      "sklearn",
      "pandas",
      "numpy",
      "PIL",
      "cv2",
      "selenium",
      "pydantic",
      "fastapi",
      "uvicorn",
      "jinja2",
      "certifi",
      "charset_normalizer",
      "urllib3",
      "requests",
      "openai",
      "anthropic",
      "langchain",
      "langchain_core",
      "sentence_transformers",
      "browser_use",
      "fake_useragent",
      "lightrag"
    ],
    "collect_binaries": [
      "torch",
      "cv2",
      "sklearn",
      "scipy",
      "numpy"
    ],
    "collect_submodules": [
      "transformers",
      "langchain",
      "langchain_community",
      "langchain_core",
      "pydantic"
    ],
    "qt_plugins": [
      "platforms",
      "imageformats",
      "iconengines",
      "styles",
      "webengine",
      "webenginewidgets"
    ],
    "collect_all": [
      "PySide6"
    ],
    "force_includes": [
      "jaraco.text",
      "jaraco.functools",
      "jaraco.context",
      "jaraco.collections",
      "jaraco.itertools",
      "jaraco.classes",
      "jaraco.logging",
      "jaraco.path",
      "jaraco.stream",
      "pkg_resources",
      "setuptools.dist",
      "importlib.metadata",
      "importlib_metadata"
    ],
    "excludes": [
      "jupyter",
      "pytest",
      "test",
      "testing",
      "pip",
      "wheel",
      "jupyter_client",
      "jupyter_core",
      "django",
      "pysqlite2",
      "MySQLdb",
      "numpy.tests",
      "pandas.tests",
      "sklearn.tests",
      "PIL.tests",
      "email.test",
      "matplotlib.tests",
      "scipy.tests",
      "IPython",
      "notebook",
      "ensurepip",
      "venv"
    ],
    "optimization": {
      "onedir_mode": true,
      "lazy_imports": true,
      "exclude_unused_modules": true,
      "compress_binaries": true,
      "strip_debug_info": true,
      "optimize_imports": true,
      "bundle_identifier": "com.ecan.app"
    }
  },
  "build_modes": {
    "dev": {
      "debug": true,
      "console": true,
      "clean": false,
      "optimize": false,
      "onefile": false,
      "use_cache": false,
      "parallel": true,
      "strip_debug": false,
      "collect_data": "all",
      "collect_binaries": "all",
      "collect_submodules": "all"
    },
    "fast": {
      "debug": false,
      "console": false,
      "clean": false,
      "optimize": true,
      "onefile": false,
      "use_cache": true,
      "parallel": true,
      "strip_debug": true,
      "collect_data": "all",
      "collect_binaries": "all",
      "collect_submodules": "all"
    },
    "prod": {
      "debug": false,
      "console": false,
      "clean": true,
      "optimize": true,
      "onefile": true,
      "use_cache": false,
      "parallel": true,
      "strip_debug": true,
      "collect_data": "all",
      "collect_binaries": "all",
      "collect_submodules": "all",
      "compression": "zlib",
      "upx_compress": false,
      "exclude_tk": false,
      "exclude_matplotlib": false,
      "lazy_imports": true,
      "runtime_tmpdir": {"windows": "C:/ecbot_cache", "macos": "~/Library/Caches/ecbot_cache", "linux": "/tmp/ecbot_cache"},
      "bootloader_ignore_signals": false,
      "disable_windowed_traceback": false
    },
    "onefile": {
      "debug": false,
      "console": false,
      "clean": true,
      "optimize": true,
      "onefile": true,
      "use_cache": false,
      "parallel": true,
      "strip_debug": true,
      "collect_data": "all",
      "collect_binaries": "all",
      "collect_submodules": "all",
      "compression": "zlib",
      "upx_compress": false,
      "lazy_imports": true,
      "runtime_tmpdir": "C:/ecbot_cache"
    },
    "onefile_alt": {
      "debug": false,
      "console": false,
      "clean": true,
      "optimize": true,
      "onefile": true,
      "use_cache": false,
      "parallel": true,
      "strip_debug": true,
      "collect_data": "minimal",
      "collect_binaries": "minimal",
      "compression": "lzma",
      "upx_compress": true,
      "exclude_tk": true,
      "exclude_matplotlib": true,
      "lazy_imports": true,
      "runtime_tmpdir": null,
      "bootloader_ignore_signals": false,
      "disable_windowed_traceback": false
    }
  },
  "installer": {
    "enabled": true,
    "app_name": "eCan",
    "app_version": "1.0.0",
    "app_publisher": "eCan.AI Team",
