    "modes": {
      "dev": {
        "debug": true,
        "console": true,
        "onefile": false
      },
      "fast": {
        "debug": true,
        "console": true,
        "onefile": false
      },
      "prod": {
        "debug": false,
        "console": false,
        "onefile": false,
        "runtime_tmpdir": {
          "windows": "C:/ecan_cache",
          "macos": "~/Library/Caches/ecan_cache",
          "linux": "/tmp/ecan_cache"
        }
      }
    }
  },
  "platforms": {
    "macos": {
      "codesign": {
        "enabled": false,
        "identity": "Developer ID Application: eCan AI Developer",
        "entitlements": "",
        "exclude_patterns": [
          "**/ms-playwright/**/chromium*/chrome-*/Chromium.app/**",
          "**/ms-playwright/**/chrome*",
          "**/QtWebEngineProcess.app/**"
        ]
      },
      "frameworks": [
        "QtWebEngineCore",
        "QtWebEngineWidgets"
      ],
      "dylib_fixes": [
        "cv2",
        "numpy",
        "scipy",
        "cryptography"
      ],
      "permissions": [
        "screen_recording",
        "accessibility",
        "camera",
        "microphone"
      ],
      "architecture": {
        "arm64": {
          "notes": "Apple Silicon (M1/M2/M3) native support",
          "playwright_platform": "mac-arm64",
          "python_requirement": "arm64 native Python (not Rosetta 2)",
          "special_handling": [
            "Ensure arm64 Chromium browser",
            "Native arm64 dependencies",
            "Universal Binary support for mixed architectures"
          ]
        },
        "x86_64": {
          "notes": "Intel Mac support",
          "playwright_platform": "mac-x64",
          "python_requirement": "x86_64 Python",
          "special_handling": [
            "Intel Chromium browser",
            "x86_64 dependencies"
          ]
        }
      }
    },
    "windows": {
      "sign": {
        "enabled": false,
        "tool": "signtool",
        "certificate": "",
        "password": ""
      }
    },
    "linux": {
      "appimage": {
        "enabled": false
      }
    }
  },
  "installer": {
    "enabled": true,
    "app_name": "eCan",
    "app_version": "1.0.0",
    "app_publisher": "eCan.AI Team",
    "default_dir": "{autopf}\\\\eCan",
    "default_group": "eCan",
    "privileges_required": "lowest",
    "compression": "lzma",
    "solid_compression": false,
    "internal_compress_level": "normal",
    "compression_modes": {
      "dev": {
        "compression": "zip",
        "solid_compression": false,
        "internal_compress_level": "normal"
      },
      "prod": {
        "compression": "lzma",
        "solid_compression": true,
        "internal_compress_level": "normal"
      },
      "fast": {
        "compression": "zip",
        "solid_compression": false,
        "internal_compress_level": "fast"
      }
    },
    "windows": {
      "app_id": "{6E1CCB74-1C0D-4333-9F20-2E4F2AF3F4A1}",
      "publisher": "eCan Team",
      "publisher_url": "https://github.com/ecan-team",
