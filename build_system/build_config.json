{
  "app_info": {
    "name": "eCan",
    "version": "1.0.0",
    "description": "eCan AI Assistant - Intelligent automation platform",
    "author": "eCan.AI Team",
    "main_script": "main.py",
    "icon_macos": "eCan.icns",
    "icon_windows": "eCan.ico",
    "console": false,
    "debug": false
  },
  "data_files": {
    "directories": [
      "resource",
      "config",
      "bot",
      "gui",
      "common",
      "utils",
      "agent",
      "knowledge",
      "settings",
      "skills",
      "telemetry",
      "gui_v2/dist",
      "ota",
      "third_party/ms-playwright"
    ],
    "files": [
      "app_context.py",
      "eCan.ico",
      "eCan.icns",
      "ecbot.qm",
      "ecbot_zh.qm",
      "role.json"
    ]
  },
      "pyinstaller": {
      "collect_all": [
        "PySide6",
        "browser_use",
        "agent",
        "crawl4ai",
        "playwright",
        "langchain",
        "langgraph",
        "langchain_openai",
        "langchain_mcp_adapters",
        "mcp"
      ],
      "collect_data_only": [
        "browser_use",
        "browser_use.browser",
        "browser_use.agent",
        "browser_use.agent.prompts",
        "lightrag",
        "lightrag.api",
        "tiktoken",
        "tiktoken_ext",
        "tiktoken_ext.openai_public"
      ],
      "force_includes": [
        "pkg_resources",
        "setuptools.dist",
        "importlib.metadata",
        "importlib_metadata"
      ],
      "force_hiddenimports": [
        "PySide6.QtWebEngineCore",
        "PySide6.QtWebEngineWidgets",
        "PySide6.QtWebEngine",
        "PySide6.QtWebChannel",
        "PySide6.QtWebSockets",
        "PySide6.QtWebView",

        "lightrag.api",
        "lightrag.api.config",
        "lightrag.api.lightrag_server",

        "jaraco.text",
        "jaraco.functools",
        "jaraco.context",
        "jaraco.collections",
        "jaraco.itertools",
        "jaraco.classes",
        "jaraco.logging",
        "jaraco.path",
        "jaraco.stream",
        "browser_use",
        "browser_use.agent",
        "browser_use.agent.prompts",
        "browser_use.browser",
        "browser_use.llm",
        "playwright",
        "playwright.async_api",
        "playwright.sync_api",
        "langchain",
        "langchain_core",
        "langgraph",
      "langchain_openai",
      "langchain_mcp_adapters",
      "mcp",
      "tiktoken_ext.openai_public"
    ],
    "codesign_exclude": [
      "**/ms-playwright/**/chromium*/chrome-*/Chromium.app/**",
      "**/ms-playwright/**/chrome*"
    ],
    "force_pre_safe": [
      "lightrag",
      "lightrag.api",
      "lightrag.api.config",
      "jaraco",
      "jaraco.text",
      "jaraco.functools",
      "more_itertools"
