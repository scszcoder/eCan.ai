      "app_name": "eCan",
      "app_version": "1.0.0",
      "copyright": "Copyright Â© 2025 eCan.AI Team",
      "install_location": "/Applications",
      "create_desktop_shortcut": true,
      "create_launchpad_shortcut": true,
      "min_os_version": "11.0",
      "codesign": {
        "enabled": false,
        "identity": "Developer ID Application: eCan AI Developer",
        "entitlements": ""
      },
      "notarization": {
        "enabled": false,
        "apple_id": "",
        "team_id": "",
        "app_password": ""
      },
      "dylib_fixes": [
        "cv2",
        "numpy",
        "scipy",
        "cryptography"
      ],
      "permissions": [
        "screen_recording",
        "accessibility",
        "camera",
        "microphone"
