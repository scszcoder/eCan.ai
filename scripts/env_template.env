# Gemini API Key (Google)
GEMINI_API_KEY=your_gemini_api_key_here

# Basic Configuration
# Log Level (DEBUG/INFO/WARNING/ERROR)
LOG_LEVEL=INFO

# Debug Mode
DEBUG_MODE=false
